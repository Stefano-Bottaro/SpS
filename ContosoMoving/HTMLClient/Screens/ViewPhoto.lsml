                Kind="Command">
                <ContentItem.Attributes>
                  <DisplayName
                    Value="Modifica" />
                </ContentItem.Attributes>
                <ContentItem.PropertySources>
                  <ControlPropertySource
                    Property="Microsoft.LightSwitch.MobileWeb:RootControl/Properties[Tap]">
                    <ScreenExpressionTree>
                      <ChainExpression>
                        <CallExpression
                          Target="!module/Methods[showAddEditPhoto]">
                          <ArgumentExpression
                            Parameter="Photo">
                            <ChainExpression>
                              <MemberExpression
                                Member="ViewPhoto/Properties[Photo]" />
                            </ChainExpression>
                          </ArgumentExpression>
                        </CallExpression>
                      </ChainExpression>
                    </ScreenExpressionTree>
                  </ControlPropertySource>
                  <ControlPropertySource
