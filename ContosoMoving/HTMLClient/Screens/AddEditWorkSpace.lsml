  <Application.Methods>
    <ApplicationMethod
      Name="showAddEditWorkSpace">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="AddEditWorkSpace" />
      </ApplicationMethod.Attributes>
      <ApplicationMethodParameter
        Name="WorkSpace"
        ParameterType="WorkSpace" />
    </ApplicationMethod>
  </Application.Methods>
</Application>