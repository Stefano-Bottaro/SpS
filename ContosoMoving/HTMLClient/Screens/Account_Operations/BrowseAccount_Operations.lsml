          View="Microsoft.LightSwitch.MobileWeb:TabPages">
          <ContentItem
            Name="Account_OperationList"
            Kind="Group">
            <ContentItem
              Name="Account_Operations"
              Kind="Collection"
              DataType="CollectionView$Account_Operation"
              View="Microsoft.LightSwitch.MobileWeb:TileList">
              <ContentItem.Attributes>
                <DisplayName
                  Value="Account_Operations" />
              </ContentItem.Attributes>
              <ContentItem.DataSource>
                <ScreenExpressionTree>
                  <ChainExpression>
                    <MemberExpression
                      Member="BrowseAccount_Operations/Properties[Account_Operations]" />
                  </ChainExpression>
                </ScreenExpressionTree>
              </ContentItem.DataSource>
