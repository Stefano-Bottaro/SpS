  <Application.Methods>
    <ApplicationMethod
      Name="showAddEditAgent">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="AddEditAgent" />
      </ApplicationMethod.Attributes>
      <ApplicationMethodParameter
        Name="Agent"
        ParameterType="Agent" />
