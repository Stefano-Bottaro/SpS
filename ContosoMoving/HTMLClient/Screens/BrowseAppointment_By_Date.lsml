        </ScreenProperty.Attributes>
      </ScreenProperty>
      <ScreenProperty
        Name="DataEnd"
        PropertyType=":DateTime">
        <ScreenProperty.Attributes>
          <Required />
        </ScreenProperty.Attributes>
      </ScreenProperty>
      <ScreenProperty
        Name="Month"
        PropertyType=":String?" />
      <ScreenProperty
        Name="Day"
        PropertyType=":String?" />
      <ScreenProperty
        Name="Hour"
        PropertyType=":String?" />
      <ScreenProperty
        Name="dayString"
        PropertyType=":String?" />
      <ScreenProperty
        Name="AppointmentDataStart"
        PropertyType=":DateTime?" />
      <ScreenProperty
        Name="AppointmentDataEnd"
        PropertyType=":DateTime" />
    </Screen.Properties>
  </Screen>
  <Application.Methods>
    <ApplicationMethod
      Name="showBrowseAppointment_By_Date">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="BrowseAppointment_By_Date" />
      </ApplicationMethod.Attributes>
    </ApplicationMethod>
  </Application.Methods>
</Application>