  <Application.Methods>
    <ApplicationMethod
      Name="showNavigator">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="Navigator" />
      </ApplicationMethod.Attributes>
    </ApplicationMethod>
  </Application.Methods>
</Application>